<Page xmlns="http://schemas.nativescript.org/tns.xsd" loaded="pageLoaded">
  <StackLayout>
    <Label text="{{ message }}" class="message" textWrap="true"/>
    <Button text="Register For Notifications" tap="{{ registerClicked }}"></Button>
    <Button text="Unregister" tap="{{ unregisterClicked }}"></Button>
  </StackLayout>
</Page>
